{
  "name": "MLSSDK",
  "version": "1.3.9",
  "summary": "An SDK for MyCujoo Live Services to help build an amazing video experience",
  "description": "This SDK is intended for customers of MyCujoo Live Services. It offers a video player, overlays, analytics and more. It has an extension for IMA as well.",
  "homepage": "https://mls.mycujoo.tv",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Empower Sports B.V.": ""
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "11.0"
  },
  "source": {
    "git": "https://github.com/mycujoo/mls-ios.git",
    "tag": "1.3.9"
  },
  "default_subspecs": "Core",
  "swift_versions": "5.0",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Sources/Core/App/**/*.swift",
        "Sources/Core/Data/**/*.swift",
        "Sources/Core/Domain/**/*.swift"
      ],
      "ios": {
        "resource_bundles": {
          "MLSResources": [
            "Sources/Core/Resources/**/*.{strings}",
            "Sources/Core/Resources/**/ios.xcassets",
            "Sources/Core/Resources/fonts/*.ttf"
          ]
        }
      },
      "tvos": {
        "resource_bundles": {
          "MLSResources": [
            "Sources/Core/Resources/**/*.{strings}",
            "Sources/Core/Resources/**/tvos.xcassets",
            "Sources/Core/Resources/fonts/*.ttf"
          ]
        }
      },
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "UIKit"
      ],
      "dependencies": {
        "YouboraLib": [

        ],
        "YouboraAVPlayerAdapter": [

        ],
        "Alamofire": [
          "~> 5.0"
        ],
        "Moya": [
          "~> 14.0"
        ]
      }
    },
    {
      "name": "IMA",
      "ios": {
        "source_files": [
          "Sources/IMA/Shared/**/*.swift",
          "Sources/IMA/iOS/**/*.swift"
        ],
        "dependencies": {
          "GoogleAds-IMA-iOS-SDK": [
            "~> 3.14"
          ]
        }
      },
      "tvos": {
        "source_files": [
          "Sources/IMA/Shared/**/*.swift",
          "Sources/IMA/tvOS/**/*.swift"
        ],
        "dependencies": {
          "GoogleAds-IMA-tvOS-SDK": [
            "~> 4.2"
          ]
        }
      },
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "UIKit"
      ],
      "dependencies": {
        "MLSSDK/Core": [

        ]
      }
    },
    {
      "name": "Cast",
      "ios": {
        "source_files": "Sources/Cast/**/*.swift",
        "dependencies": {
          "google-cast-sdk": [
            "~> 4.5"
          ]
        }
      },
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "UIKit"
      ],
      "dependencies": {
        "MLSSDK/Core": [

        ]
      }
    },
    {
      "name": "Annotations",
      "ios": {
        "source_files": [
          "Sources/Annotations/Shared/**/*.swift",
          "Sources/Annotations/iOS/**/*.swift"
        ]
      },
      "tvos": {
        "source_files": [
          "Sources/Annotations/Shared/**/*.swift",
          "Sources/Annotations/tvOS/**/*.swift"
        ]
      },
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "UIKit"
      ],
      "dependencies": {
        "MLSSDK/Core": [

        ]
      }
    },
    {
      "name": "IAP",
      "ios": {
        "source_files": "Sources/IAP/**/*.swift"
      },
      "frameworks": [
        "Foundation",
        "StoreKit",
        "UIKit"
      ],
      "dependencies": {
        "MLSSDK/Core": [

        ]
      }
    }
  ],
  "swift_version": "5.0"
}
